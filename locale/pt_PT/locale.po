msgid "plugins.generic.frontEndCache.name"
msgstr "Plugin de cache de front-end"

msgid "plugins.generic.frontEndCache.description"
msgstr "Este plugin adiciona cache às páginas principais da aplicação."

msgid "plugins.generic.frontEndCache.general"
msgstr "Definições gerais"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Usar cabeçalho de cache: quando ativado, o plugin enviará cabeçalhos de cache para o navegador"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Usar compressão: Quando ativado, o plugin tentará compactar o conteúdo usando gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Usar estatísticas: Quando ativado, o plugin tentará alimentar as estatísticas da aplicação ao servir uma página em cache"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Cache CSS: Quando ativado, o plugin armazenará em cache o conteúdo CSS dinâmico"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Tempo de vida de cada página em cache em segundos: Após este tempo, a página em cache será invalidada e regenerada"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Lista de prefixos para instruir o plugin sobre quais páginas devem ser armazenadas em cache"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Lista de páginas/operações onde a cache não deve ser ativada"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Selecione os contextos para os quais deseja limpar a cache"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Páginas partilhadas"

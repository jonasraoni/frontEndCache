msgid "plugins.generic.frontEndCache.name"
msgstr "Front End Cache Plugin"

msgid "plugins.generic.frontEndCache.description"
msgstr "This plugin adds cache to the main pages of the application."

msgid "plugins.generic.frontEndCache.general"
msgstr "General settings"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Use cache header: when enabled the plugin will send cache headers to the browser"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Use compression: When enabled the plugin will attempt to compact the content using gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Use statistics: When enabled the plugin will attempt to feed the application statistics when serving a cached page"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Cache CSS: When enabled the plugin will cache dynamic CSS content"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Time to live of each cached page in seconds: After the time is over, the cached page will be invalidated and regenerated"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "List of prefixes to instruct the plugin which pages should be cacheable"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "List of pages/operations where the cache shouldn't be enabled"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Select the contexts for which you would like the cache to be cleared"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Shared pages"

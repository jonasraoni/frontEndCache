msgid "plugins.generic.frontEndCache.name"
msgstr "Front-end skyndiminni viðbót"

msgid "plugins.generic.frontEndCache.description"
msgstr "Þessi viðbót bætir við skyndiminni fyrir aðalsíður forritsins."

msgid "plugins.generic.frontEndCache.general"
msgstr "Almennar stillingar"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Nota skyndiminni haus: þegar virkt, sendir viðbótin skyndiminni hausa í vafrann"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Nota þjöppun: Þegar virkt, reynir viðbótin að þjappa efni með gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Nota tölfræði: Þegar virkt, reynir viðbótin að uppfæra tölfræði forritsins þegar skyndiminni síða er afgreidd"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Skyndiminni CSS: Þegar virkt, geymir viðbótin dynamic CSS efni í skyndiminni"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Líftími hverrar skyndiminni síðu í sekúndum: Eftir þennan tíma verður síðan ógild og endurgerð"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Listi yfir forskeyti til að leiðbeina viðbótinni um hvaða síður eigi að geyma í skyndiminni"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Listi yfir síður/aðgerðir þar sem skyndiminni á ekki að vera virkt"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Veldu samhengi sem þú vilt hreinsa skyndiminni fyrir"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Sameiginlegar síður"

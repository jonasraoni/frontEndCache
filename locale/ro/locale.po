msgid "plugins.generic.frontEndCache.name"
msgstr "Plugin cache front-end"

msgid "plugins.generic.frontEndCache.description"
msgstr "Acest plugin adaugă cache la paginile principale ale aplicației."

msgid "plugins.generic.frontEndCache.general"
msgstr "Setări generale"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Utilizează antet cache: când este activat, plugin-ul va trimite antete cache către browser"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Utilizează compresie: Când este activat, plugin-ul va încerca să comprime conținutul folosind gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Utilizează statistici: Când este activat, plugin-ul va încerca să actualizeze statisticile aplicației când deservește o pagină din cache"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Cache CSS: Când este activat, plugin-ul va stoca în cache conținutul CSS dinamic"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Durata de viață a fiecărei pagini din cache în secunde: După această perioadă, pagina din cache va fi invalidată și regenerată"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Listă de prefixe pentru a indica plugin-ului care pagini ar trebui să fie stocate în cache"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Listă de pagini/operații unde cache-ul nu ar trebui activat"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Selectați contextele pentru care doriți să ștergeți cache-ul"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Pagini partajate"

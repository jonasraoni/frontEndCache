msgid "plugins.generic.frontEndCache.name"
msgstr "Plugin per la cache front-end"

msgid "plugins.generic.frontEndCache.description"
msgstr "Questo plugin aggiunge la cache alle pagine principali dell'applicazione."

msgid "plugins.generic.frontEndCache.general"
msgstr "Impostazioni generali"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Usa intestazione cache: quando abilitato, il plugin invierà intestazioni cache al browser"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Usa compressione: Quando abilitato, il plugin tenterà di comprimere il contenuto usando gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Usa statistiche: Quando abilitato, il plugin tenterà di aggiornare le statistiche dell'applicazione quando serve una pagina in cache"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Cache CSS: Quando abilitato, il plugin memorizzerà nella cache il contenuto CSS dinamico"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Durata di ogni pagina in cache in secondi: Dopo questo tempo, la pagina in cache sarà invalidata e rigenerata"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Elenco di prefissi per indicare al plugin quali pagine devono essere memorizzate nella cache"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Elenco di pagine/operazioni in cui la cache non deve essere abilitata"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Seleziona i contesti per i quali desideri cancellare la cache"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Pagine condivise"

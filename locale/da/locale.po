msgid "plugins.generic.frontEndCache.name"
msgstr "Front-end cache plugin"

msgid "plugins.generic.frontEndCache.description"
msgstr "Dette plugin tilføjer cache til applikationens hovedsider."

msgid "plugins.generic.frontEndCache.general"
msgstr "Generelle indstillinger"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Brug cache-header: når aktiveret, vil pluginet sende cache-headere til browseren"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Brug komprimering: Når aktiveret, vil pluginet forsøge at komprimere indholdet ved hjælp af gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Brug statistikker: Når aktiveret, vil pluginet forsøge at opdatere applikationens statistikker, når en cachelagret side vises"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Cache CSS: Når aktiveret, vil pluginet cache dynamisk CSS-indhold"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Levetid for hver cachelagret side i sekunder: Efter denne tid vil den cachelagrede side blive ugyldiggjort og regenereret"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Liste over præfikser for at angive, hvilke sider der skal cachelagres"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Liste over sider/operationer, hvor cache ikke skal aktiveres"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Vælg de kontekster, du vil rydde cachen for"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Delte sider"

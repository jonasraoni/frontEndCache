msgid "plugins.generic.frontEndCache.name"
msgstr "Plugin Cache Front-End"

msgid "plugins.generic.frontEndCache.description"
msgstr "Plugin ini menambahkan cache ke halaman utama aplikasi."

msgid "plugins.generic.frontEndCache.general"
msgstr "Pengaturan umum"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Gunakan header cache: ketika diaktifkan, plugin akan mengirim header cache ke browser"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Gunakan kompresi: Ketika diaktifkan, plugin akan mencoba mengompresi konten menggunakan gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Gunakan statistik: Ketika diaktifkan, plugin akan mencoba memperbarui statistik aplikasi saat menyajikan halaman cache"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Cache CSS: Ketika diaktifkan, plugin akan menyimpan konten CSS dinamis dalam cache"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Masa hidup setiap halaman cache dalam detik: Setelah waktu ini, halaman cache akan dinonaktifkan dan dibuat ulang"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Daftar awalan untuk memberi tahu plugin halaman mana yang harus di-cache"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Daftar halaman/operasi di mana cache tidak boleh diaktifkan"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Pilih konteks yang ingin Anda hapus cache-nya"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Halaman bersama"

msgid "plugins.generic.frontEndCache.name"
msgstr "Front-end keshi plagini"

msgid "plugins.generic.frontEndCache.description"
msgstr "Ushbu plagin dasturning asosiy sahifalariga keshlash qo'shadi."

msgid "plugins.generic.frontEndCache.general"
msgstr "Umumiy sozlamalar"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Kesh sarlavhasidan foydalaning: yoqilganda, plagin brauzerga kesh sarlavhalarini yuboradi"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Siqishdan foydalaning: Yoqilganda, plagin gzip yordamida kontentni siqishga harakat qiladi"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Statistikadan foydalaning: Yoqilganda, plagin keshlangan sahifani xizmat qilishda dastur statistikasini yangilashga harakat qiladi"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "CSS keshi: Yoqilganda, plagin dinamik CSS kontentini keshlaydi"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Har bir keshlangan sahifaning yashash muddati (soniyada): Bu vaqtdan so'ng, keshlangan sahifa bekor qilinadi va qayta yaratiladi"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Plaginga qaysi sahifalarni keshlash kerakligini ko'rsatadigan prefikslar ro'yxati"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Keshlashni yoqish shart bo'lmagan sahifalar/amallar ro'yxati"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Keshini tozalamoqchi bo'lgan kontekstlarni tanlang"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Ulashilgan sahifalar"

msgid "plugins.generic.frontEndCache.name"
msgstr "Doplnok front-end cache"

msgid "plugins.generic.frontEndCache.description"
msgstr "Tento doplnok pridáva cache k hlavným stránkam aplikácie."

msgid "plugins.generic.frontEndCache.general"
msgstr "Všeobecné nastavenia"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Použiť hlavičku cache: keď je povolené, doplnok bude posielať hlavičky cache do prehliadača"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Použiť kompresiu: Keď je povolené, doplnok sa pokúsi komprimovať obsah pomocou gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Použiť štatistiky: Keď je povolené, doplnok sa pokúsi aktualizovať štatistiky aplikácie pri poskytovaní stránky z cache"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Cache CSS: Keď je povolené, doplnok bude ukladať dynamický CSS obsah do cache"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Životnosť každej stránky v cache v sekundách: Po tomto čase bude stránka v cache zneplatnená a znovu vygenerovaná"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Zoznam predpon na určenie, ktoré stránky by mali byť ukladané do cache"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Zoznam stránok/operácií, pre ktoré by cache nemala byť povolená"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Vyberte kontexty, pre ktoré chcete vyčistiť cache"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Zdieľané stránky"

msgid "plugins.generic.frontEndCache.name"
msgstr "Vtičnik za predpomnilnik sprednjega dela"

msgid "plugins.generic.frontEndCache.description"
msgstr "Ta vtičnik doda predpomnilnik glavnim stranem aplikacije."

msgid "plugins.generic.frontEndCache.general"
msgstr "Splošne nastavitve"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Uporabi glavo predpomnilnika: ko je omogočeno, bo vtičnik pošiljal glave predpomnilnika brskalniku"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Uporabi stiskanje: Ko je omogočeno, bo vtičnik poskušal stisniti vsebino z uporabo gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Uporabi statistiko: Ko je omogočeno, bo vtičnik poskušal posodobiti statistiko aplikacije pri serviranju strani iz predpomnilnika"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Predpomnilnik CSS: Ko je omogočeno, bo vtičnik shranjeval dinamično CSS vsebino v predpomnilnik"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Življenjska doba vsake strani v predpomnilniku v sekundah: Po tem času bo stran v predpomnilniku razveljavljena in ponovno ustvarjena"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Seznam predpon za navodila vtičniku, katere strani naj se shranjujejo v predpomnilnik"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Seznam strani/operacij, kjer predpomnilnik ne sme biti omogočen"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Izberite kontekste, za katere želite počistiti predpomnilnik"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Skupne strani"

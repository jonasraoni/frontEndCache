msgid "plugins.generic.frontEndCache.name"
msgstr "Front-end hurtigbuffer-tillegg"

msgid "plugins.generic.frontEndCache.description"
msgstr "Dette tillegget legger til hurtigbuffer på applikasjonens hovedsider."

msgid "plugins.generic.frontEndCache.general"
msgstr "Generelle innstillinger"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Bruk hurtigbuffer-hode: når aktivert, vil tillegget sende hurtigbuffer-hoder til nettleseren"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Bruk komprimering: Når aktivert, vil tillegget forsøke å komprimere innholdet ved hjelp av gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Bruk statistikk: Når aktivert, vil tillegget forsøke å oppdatere applikasjonens statistikk når det serverer en hurtigbuffret side"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Hurtigbuffer CSS: Når aktivert, vil tillegget hurtigbuffre dynamisk CSS-innhold"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Levetid for hver hurtigbuffret side i sekunder: Etter denne tiden vil den hurtigbuffrede siden bli ugyldig og regenerert"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Liste over prefikser for å veilede tillegget om hvilke sider som skal hurtigbuffres"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Liste over sider/operasjoner hvor hurtigbuffer ikke skal aktiveres"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Velg kontekstene du vil tømme hurtigbuffer for"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Delte sider"

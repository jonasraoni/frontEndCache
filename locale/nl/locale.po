msgid "plugins.generic.frontEndCache.name"
msgstr "Front-end cache plugin"

msgid "plugins.generic.frontEndCache.description"
msgstr "Deze plugin voegt caching toe aan de hoofdpagina's van de applicatie."

msgid "plugins.generic.frontEndCache.general"
msgstr "Algemene instellingen"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Gebruik cache-header: indien ingeschakeld, stuurt de plugin cache-headers naar de browser"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Gebruik compressie: Indien ingeschakeld, probeert de plugin de inhoud te comprimeren met gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Gebruik statistieken: Indien ingeschakeld, probeert de plugin de applicatiestatistieken bij te werken bij het serveren van een gecachete pagina"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Cache CSS: Indien ingeschakeld, cached de plugin dynamische CSS-inhoud"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Levensduur van elke gecachete pagina in seconden: Na deze tijd wordt de gecachete pagina ongeldig en opnieuw gegenereerd"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Lijst van voorvoegsels om de plugin te vertellen welke pagina's gecached moeten worden"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Lijst van pagina's/bewerkingen waar caching niet moet worden ingeschakeld"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Selecteer de contexten waarvoor u de cache wilt wissen"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Gedeelde pagina's"

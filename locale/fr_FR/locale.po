msgid "plugins.generic.frontEndCache.name"
msgstr "Plugin de caché de front-end"

msgid "plugins.generic.frontEndCache.description"
msgstr "Este plugin añade caché a las páginas principales de la aplicación."

msgid "plugins.generic.frontEndCache.general"
msgstr "Configuración general"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Usar cabecera de caché: cuando está activado, el plugin enviará cabeceras de caché al navegador"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Usar compresión: Cuando está activado, el plugin intentará compactar el contenido usando gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Usar estadísticas: Cuando está activado, el plugin intentará alimentar las estadísticas de la aplicación al servir una página en caché"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Cachear CSS: Cuando está activado, el plugin cacheará contenido CSS dinámico"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Tiempo de vida de cada página cacheada en segundos: Después de este tiempo, la página cacheada será invalidada y regenerada"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Lista de prefijos para indicar al plugin qué páginas deben ser cacheables"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Lista de páginas/operaciones donde no se debe activar la caché"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Seleccione los contextos para los que desea borrar la caché"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Páginas compartidas"

msgid "plugins.generic.frontEndCache.name"
msgstr "Dodatak za keširanje front-enda"

msgid "plugins.generic.frontEndCache.description"
msgstr "Ovaj dodatak dodaje keš glavnim stranama aplikacije."

msgid "plugins.generic.frontEndCache.general"
msgstr "Opšte postavke"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Koristi zaglavlje keša: kada je uključeno, dodatak će slati zaglavlja keša pretraživaču"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Koristi kompresiju: Kada je uključeno, dodatak će pokušati da kompresuje sadržaj koristeći gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Koristi statistike: Kada je uključeno, dodatak će pokušati da ažurira statistike aplikacije prilikom serviranja stranice iz keša"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Keširaj CSS: Kada je uključeno, dodatak će keširati dinamički CSS sadržaj"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Vreme života svake keširane strane u sekundama: Posle ovog vremena, keširana strana će biti poništena i ponovo generisana"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Lista prefiksa za upućivanje dodatka koje strane treba keširati"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Lista strana/operacija gde keš ne treba da bude uključen"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Izaberite kontekste za koje želite da očistite keš"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Deljene strane"

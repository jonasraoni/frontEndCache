msgid "plugins.generic.frontEndCache.name"
msgstr "Complemento de caché front-end"

msgid "plugins.generic.frontEndCache.description"
msgstr "Este complemento engade caché ás páxinas principais da aplicación."

msgid "plugins.generic.frontEndCache.general"
msgstr "Configuración xeral"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Usar cabeceira de caché: cando está activado, o complemento enviará cabeceiras de caché ao navegador"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Usar compresión: Cando está activado, o complemento tentará comprimir o contido usando gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Usar estatísticas: Cando está activado, o complemento tentará actualizar as estatísticas da aplicación ao servir unha páxina en caché"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Caché CSS: Cando está activado, o complemento almacenará en caché o contido CSS dinámico"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Tempo de vida de cada páxina en caché en segundos: Despois deste tempo, a páxina en caché invalidarase e xerarase de novo"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Lista de prefixos para indicar ao complemento que páxinas deben almacenarse en caché"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Lista de páxinas/operacións onde non se debe activar a caché"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Seleccione os contextos para os que quere borrar a caché"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Páxinas compartidas"

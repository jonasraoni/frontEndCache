msgid "plugins.generic.frontEndCache.name"
msgstr "Plugin pro front-end cache"

msgid "plugins.generic.frontEndCache.description"
msgstr "Tento plugin přidává cache k hlavním stránkám aplikace."

msgid "plugins.generic.frontEndCache.general"
msgstr "Obecná nastavení"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Použít cache hlavičku: když je povoleno, plugin bude posílat cache hlavičky do prohlížeče"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Použít kompresi: Když je povoleno, plugin se pokusí komprimovat obsah pomocí gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Použít statistiky: Když je povoleno, plugin se pokusí aktualizovat statistiky aplikace při zobrazení stránky z cache"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Cache CSS: Když je povoleno, plugin bude ukládat dynamický CSS obsah do cache"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Životnost každé stránky v cache v sekundách: Po uplynutí této doby bude stránka v cache zneplatněna a znovu vygenerována"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Seznam prefixů, které určují, které stránky by měly být ukládány do cache"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Seznam stránek/operací, pro které by cache neměla být povolena"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Vyberte kontexty, pro které chcete vymazat cache"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Sdílené stránky"

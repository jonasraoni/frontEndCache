msgid "plugins.generic.frontEndCache.name"
msgstr "Dodatak za prednju predmemoriju"

msgid "plugins.generic.frontEndCache.description"
msgstr "Ovaj dodatak dodaje predmemoriju glavnim stranicama aplikacije."

msgid "plugins.generic.frontEndCache.general"
msgstr "Opće postavke"

msgid "plugins.generic.frontEndCache.useCacheHeader"
msgstr "Koristi zaglavlje predmemorije: kada je omogućeno, dodatak će slati zaglavlja predmemorije pregledniku"

msgid "plugins.generic.frontEndCache.useCompression"
msgstr "Koristi kompresiju: Kada je omogućeno, dodatak će pokušati komprimirati sadržaj koristeći gzip"

msgid "plugins.generic.frontEndCache.useStatistics"
msgstr "Koristi statistike: Kada je omogućeno, dodatak će pokušati osvježiti statistike aplikacije prilikom posluživanja stranice iz predmemorije"

msgid "plugins.generic.frontEndCache.cacheCss"
msgstr "Predmemorija CSS-a: Kada je omogućeno, dodatak će predmemorirati dinamički CSS sadržaj"

msgid "plugins.generic.frontEndCache.timeToLiveInSeconds"
msgstr "Vrijeme života svake stranice u predmemoriji u sekundama: Nakon tog vremena, stranica u predmemoriji će biti poništena i ponovno generirana"

msgid "plugins.generic.frontEndCache.cacheablePages"
msgstr "Popis prefiksa za upućivanje dodatka koje stranice treba predmemorirati"

msgid "plugins.generic.frontEndCache.nonCacheableOperations"
msgstr "Popis stranica/operacija za koje se predmemorija ne smije koristiti"

msgid "plugins.generic.frontEndCache.clearCacheInstruction"
msgstr "Odaberite kontekste za koje želite očistiti predmemoriju"

msgid "plugins.generic.frontEndCache.sharedContent"
msgstr "Dijeljene stranice"
